   "source": [
    "def pattern(n):\n",
    "    for i in range (1,n+1):\n",
    "        for j in range (1,n-i+2):\n",
    "            print(j,end = \" \")\n",
    "        if i>1:\n",
    "            for j in range (0,2*i-3):\n",
    "                print(\"*\",end = \" \")\n",
    "        print()"
   ]
